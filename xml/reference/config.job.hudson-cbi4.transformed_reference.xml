<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description>&lt;p&gt;This job builds a headless product and a p2 repository that contains various tools to testing and analyzing different aspects of p2 repositories&lt;/p&gt;&#13;
</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.security.AuthorizationMatrixProperty>
      <permission>hudson.model.Item.Workspace:anonymous</permission>
      <permission>hudson.model.Item.Workspace:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Item.Workspace:david_williams@acm.org</permission>
      <permission>hudson.model.Item.Delete:david_williams@acm.org</permission>
      <permission>hudson.model.Run.Update:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Run.Update:david_williams@acm.org</permission>
      <permission>hudson.model.Item.Configure:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Item.Configure:david_williams@acm.org</permission>
      <permission>hudson.model.Run.Delete:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Run.Delete:david_williams@acm.org</permission>
      <permission>hudson.model.Item.Read:anonymous</permission>
      <permission>hudson.model.Item.Read:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Item.Read:david_williams@acm.org</permission>
      <permission>hudson.model.Item.Build:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Item.Build:david_williams@acm.org</permission>
      <permission>hudson.model.Item.ExtendedRead:dennis.huebner@gmail.com</permission>
      <permission>hudson.model.Item.ExtendedRead:david_williams@acm.org</permission>
    </hudson.security.AuthorizationMatrixProperty>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>5</daysToKeep>
        <numToKeep>5</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.plugins.disk__usage.DiskUsageProperty/>
    <hudson.model.ParametersDefinitionProperty>
            <parameterDefinitions>
              <hudson.model.StringParameterDefinition>
                <name>GERRIT_REFSPEC</name>
                <description>&lt;p&gt;Do not need to do anything here for a normal Gerrit build. It is here for when someone wants to trigger a build and make sure they are getting the tip of master. &lt;/p&gt;</description>
                <defaultValue>refs/heads/master</defaultValue>
              </hudson.model.StringParameterDefinition>
            </parameterDefinitions>
          </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM">
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <name>origin</name>
        <refspec>$GERRIT_REFSPEC</refspec>
        <url>file:///gitroot/cbi/org.eclipse.cbi.p2repo.analyzers</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
            <hudson.plugins.git.BranchSpec>
              <name>master</name>
            </hudson.plugins.git.BranchSpec>
          </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions>
      <hudson.plugins.git.extensions.impl.RelativeTargetDirectory>
        <relativeTargetDir>org.eclipse.cbi.p2repo.analyzers</relativeTargetDir>
      </hudson.plugins.git.extensions.impl.RelativeTargetDirectory>
      <hudson.plugins.git.extensions.impl.BuildChooserSetting>
        <buildChooser class="com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.GerritTriggerBuildChooser">
          <separator>#</separator>
        </buildChooser>
      </hudson.plugins.git.extensions.impl.BuildChooserSetting>
    </extensions>
  </scm>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <jdk>jdk1.8.0-latest</jdk>
  <triggers>
    <com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.GerritTrigger>
      <spec/>
      <gerritProjects>
            <com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.data.GerritProject>
              <compareType>ANT</compareType>
              <pattern>cbi/org.eclipse.cbi.p2repo.analyzers</pattern>
              <branches>
                <com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.data.Branch>
                  <compareType>ANT</compareType>
                  <pattern>**</pattern>
                </com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.data.Branch>
              </branches>
            </com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.data.GerritProject>
          </gerritProjects>
      <silentMode>false</silentMode>
      <escapeQuotes>true</escapeQuotes>
      <buildStartMessage/>
      <buildFailureMessage/>
      <buildSuccessfulMessage/>
      <buildUnstableMessage/>
      <buildNotBuiltMessage/>
      <buildUnsuccessfulFilepath/>
      <customUrl/>
      <triggerOnEvents>
            <com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.events.PluginPatchsetCreatedEvent/>
            <com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.events.PluginDraftPublishedEvent/>
          </triggerOnEvents>
      <dynamicTriggerConfiguration>false</dynamicTriggerConfiguration>
      <triggerConfigURL/>
      <triggerInformationAction/>
    </com.sonyericsson.hudson.plugins.gerrit.trigger.hudsontrigger.GerritTrigger>
  </triggers>
  <builders>
    <hudson.tasks.Maven>
      <targets>clean verify -e -X -Peclipse-sign</targets>
      <mavenName>apache-maven-latest</mavenName>
      <jvmOptions/>
      <pom>org.eclipse.cbi.p2repo.analyzers/releng/org.eclipse.cbi.p2repo.analyzers.parent/pom.xml</pom>
      <properties/>
      <usePrivateRepository>true</usePrivateRepository>
    </hudson.tasks.Maven>
    <hudson.tasks.Shell>
      <command>#!/usr/bin/env bash
mkdir -p output/products/
mkdir -p output/p2repo/
cp org.eclipse.cbi.p2repo.analyzers/releng/org.eclipse.cbi.p2repo.analyzers.product/target/products/*product-* output/products/
cp -r org.eclipse.cbi.p2repo.analyzers/releng/org.eclipse.cbi.p2repo.analyzers.repository/target/repository/* output/p2repo/</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/usr/bin/env bash

# After we build, we will check the repository we just 
# produced, to make sure it is basically compliant.

mkdir testRepo
tar -xf output/products/org.eclipse.cbi.p2repo.analyzers.product-linux.gtk.x86_64.tar.gz -C testRepo

./testRepo/p2analyze/p2analyze -data workspace-report -vmargs -DreportRepoDir=${WORKSPACE}/output/p2repo/ -DreportOutputDir=${WORKSPACE}/output
</command>
    </hudson.tasks.Shell>
  </builders>
